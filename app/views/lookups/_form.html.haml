%tr
  = form_for(Lookup.new) do |f|
    %td= f.text_field :value
    %td= f.text_field :descripcion
    %td
      = f.submit 'Guardar', class: "btn btn-primary"
      %span{:style => "display:none;"}= f.text_field :parent, :value => get_type_id(l_type)
      %span{:style => "display:none;"}= f.text_field :lookup_type, :value => get_type_value(l_type)
